<% if notice %>
  <div class="alert alert-info fade show m-1" role="alert">
    <%= notice %>
  </div>
<% end %>
<% if alert %>
  <div class="alert alert-warning fade show m-1" role="alert">
    <%= alert %>
  </div>
<% end %>

<% if flash[:success] %>
  <div class="alert alert-success fade show m-1" role="alert">
    <%= flash[:success] %>
  </div>
<% end %>
